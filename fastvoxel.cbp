<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="fastvoxel" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option platforms="Windows;" />
				<Option output="Release\CubicGen" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add directory=".\" />
				</Compiler>
			</Target>
			<Target title="debug">
				<Option platforms="Windows;" />
				<Option output="Debug\DebugCubicGen" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters=" -prec7.8 -iv1 -iRelease\nantes.ply -oremesh_nantes.XYZ" />
				<Compiler>
					<Add option="-g" />
					<Add directory=".\" />
				</Compiler>
			</Target>
			<Target title="Release Unix">
				<Option platforms="Unix;" />
				<Option output="Release Linux\CubicGen" prefix_auto="1" extension_auto="1" />
				<Option object_output="Release Linux\.objs" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add directory=".\" />
				</Compiler>
			</Target>
			<Target title="python_lib_windows32bits">
				<Option output="fastvoxel\_fastvoxel.pyd" prefix_auto="0" extension_auto="0" />
				<Option object_output="pythonlibobjs" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-D_WIN32" />
					<Add option="-DWIN32" />
					<Add directory=".\" />
					<Add directory="%PYTHON%\include" />
					<Add directory="C:\Python26\Lib\site-packages\numpy\core\include" />
				</Compiler>
				<Linker>
					<Add library="%PYTHON%\libs\libpython26.a" />
				</Linker>
				<ExtraCommands>
					<Add before="swig -c++ -Fstandard -python -I../ src/fastvoxel.i" />
				</ExtraCommands>
			</Target>
			<Target title="python_lib_linux">
				<Option platforms="Unix;" />
				<Option output="fastvoxel\_fastvoxel" prefix_auto="1" extension_auto="1" />
				<Option working_dir="fastvoxel" />
				<Option object_output="fastvoxel" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-fPIC" />
					<Add option="-D_UNIX" />
					<Add option="-DUNIX" />
					<Add option="-DLINUX" />
					<Add directory=".\" />
					<Add directory="\usr\include\python2.6" />
					<Add directory="\usr\include\python2.6\numpy" />
				</Compiler>
				<Linker>
					<Add library="\usr\lib64\libpython2.6.so" />
				</Linker>
				<ExtraCommands>
					<Add before="swig -c++ -Fstandard -python -I../ src/fastvoxel.i" />
					<Add after="cp fastvoxel.py fastvoxel/" />
					<Add after="python fastvoxel/test_triangle_feed.py" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="python_lib_windows64bits">
				<Option output="fastvoxel\_fastvoxel.pyd" prefix_auto="0" extension_auto="0" />
				<Option object_output="pythonlibobjs" />
				<Option type="3" />
				<Option compiler="msvc8" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="/EHs" />
					<Add option="-D_WIN64" />
					<Add option="-DWIN64" />
					<Add option="/D_WIN64" />
					<Add option="/DWIN64" />
					<Add directory="src\" />
					<Add directory="%PYTHON%\include" />
					<Add directory="%PYTHON%\Lib\site-packages\numpy\core\include" />
				</Compiler>
				<Linker>
					<Add library="%PYTHON%\libs\python27.lib" />
				</Linker>
				<ExtraCommands>
					<Add before="swig -outdir fastvoxel -c++ -Fstandard -python -Isrc/ src/fastvoxel.i" />
					<Add after="%PYTHON%/python.exe fastvoxel\test_triangle_feed.py" />
				</ExtraCommands>
			</Target>
		</Build>
